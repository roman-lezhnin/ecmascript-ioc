{
  "name": "ts-spring-di",
  "version": "1.0.0",
  "description": "A TypeScript Dependency Injection library mirroring Java Spring Framework's DI mechanism",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/roman-lezhnin/ts-spring-di.git"
  },
  "keywords": [
    "typescript",
    "DI",
    "di-container",
    "dependency-injection",
    "spring",
    "decorators"
  ],
  "author": "Roman Lezhnin <romalezhnin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "prepare": "npm run build",
    "docs": "typedoc src"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.0.3",
    "ts-mockito": "^2.6.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.0.0"
  }
}
