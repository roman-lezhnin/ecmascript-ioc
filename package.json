{
  "name": "ecmascript-ioc",
  "version": "0.3.7",
  "description": "IoC library mirroring Java Spring Framework's implementation",
  "author": "Roman Lezhnin <romalezhnin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roman-lezhnin/ecmascript-ioc.git"
  },
  "bugs": {
    "url": "https://github.com/roman-lezhnin/ecmascript-ioc/issues"
  },
  "keywords": [
    "Java",
    "Spring",
    "typescript",
    "IoC",
    "DI",
    "Inversion of Control",
    "Dependency Injection",
    "ioc container",
    "di container",
    "decorators"
  ],
  "scripts": {
    "build": "webpack",
    "prepare": "npm run build",
    "test": "jest",
    "docs": "typedoc src",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "main": "dist/ecmascript-ioc.cjs.js",
  "module": "dist/ecmascript-ioc.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ecmascript-ioc.esm.js",
      "require": "./dist/ecmascript-ioc.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.2.0",
    "eslint": "^8.57.1",
    "jest": "^29.2.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.5.1",
    "ts-mockito": "^2.6.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.6.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
