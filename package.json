{
  "name": "ecmascript-ioc",
  "version": "0.3.1",
  "description": "IoC library mirroring Java Spring Framework's implementation",
  "author": "Roman Lezhnin <romalezhnin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roman-lezhnin/ecmascript-ioc.git"
  },
  "bugs": {
    "url": "httckaps://github.com/roman-lezhnin/ecmascript-ioc/issues"
  },
  "keywords": [
    "typescript",
    "DI",
    "di-container",
    "dependency-injection",
    "spring",
    "decorators"
  ],
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && bash ./fixup",
    "prepare": "npm run build",
    "test": "jest",
    "docs": "typedoc src",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/mjs/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.2.0",
    "eslint": "^8.57.1",
    "jest": "^29.2.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.0.3",
    "ts-mockito": "^2.6.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.6.0"
  }
}
